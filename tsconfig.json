{
  "extends": "./.wxt/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@": ["./src"],
      "@/*": ["./src/*"],
      "~": ["./src"],
      "~/*": ["./src/*"],
      "@@": ["./"],
      "@@/*": ["./*"],
      "~~": ["./"],
      "~~/*": ["./*"],
      "#build/ui": ["./node_modules/@nuxt/ui/.nuxt/ui"]
    }
  },
  "include": [
    "./**/*",
    "./.wxt/wxt.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.vue",
    "./auto-imports.d.ts",
    "./components.d.ts"
  ]
}
